<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zhbit.domain.Store" table="bit_store">
        <id name="id">
            <generator class="native" />
        </id>
        <property name="storename" type ="string" length="255"/>
        <property name="productNum" column="product_num"/>
        <property name="createTime"  column="create_time" type="timestamp"/>
        <property name="adminId" column="admin_id" />
        <property name="adminName" column="admin_name" type="string" length="255"/>
        <property name="decript" type="string" length="255"/>
        <property name="address" type="string" length="255"/>
        <property name="tel" type="string" length="255"/>
        <property name="tel" type="string" length="255"/>
    </class>
</hibernate-mapping>