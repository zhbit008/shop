<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zhbit.domain.ProductCate" table="bit_product_cate">
        <id name="id">
            <generator class="native" />
        </id>
        <property name="catename" type ="string" length="255"/>
        <property name="childnum" />
        <property name="pid" />

    </class>
</hibernate-mapping>