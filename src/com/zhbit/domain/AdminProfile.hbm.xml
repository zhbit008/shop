<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zhbit.domain.AdminProfile" table="bit_admin_profile">
        <id name="adminId" column="admin_id">
            <generator class="native" />
        </id>
        <property name="key" type ="string" length="255"/>
        <property name="value" type="string" length="255"/>
    </class>
</hibernate-mapping>